a="COMPUTER"
b1=a[-8:]
print(b1)
b=a[-8: :2]
print(b)
c1=a[:8]
print(c1)
c2=a[0:]
print(c2)
c=a[0:8:2]
print(c)
d=a[:]
print(d)
e=a[::-1]
print(e)
f=a[1:-4]
print(f)
g=a[8:3:-1]
print(g)
h=a[::-1]
print(h)


